{
    "manifest_version":3,
    "name":"Tab Manager for Chrome Pratice",
    "version":"1.0.0",
    "icons":{
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "host_permissions":[
    "https://*/*"
    ],
    "permissions":["tabGroups" ],
    "action": {
        "default_popup": "popup.html"
      }
}